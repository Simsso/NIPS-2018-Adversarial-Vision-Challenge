{
    "steps": [
        {
            "name":"gcr.io/cloud-builders/docker",
            "args": [
                "build",
                "-t",
                "gcr.io/$PROJECT_ID/$TAG_NAME:$SHORT_SHA",
                "models/$TAG_NAME"
            ]
        },
        {
            "name":"gcr.io/cloud-builders/docker",
            "args": [
                "push",
                "gcr.io/$PROJECT_ID/$TAG_NAME:$SHORT_SHA"
            ]
        },
        {
            "name": "ubuntu",
            "args": ["bash", "./create_gce_instance.sh"] 
        }
    ]
}